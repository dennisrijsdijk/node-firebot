{
  "name": "@dennisrijsdijk/node-firebot",
  "version": "0.1.0",
  "description": "Abstraction over the Firebot REST API and Websocket",
  "license": "MIT",
  "author": "dennisrijsdijk",
  "type": "module",
  "main": "dist/node-firebot.cjs",
  "module": "dist/node-firebot.mjs",
  "types": "dist/node-firebot.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node-firebot.d.ts",
      "require": "./dist/node-firebot.cjs",
      "import": "./dist/node-firebot.mjs"
    }
  },
  "files": [
    "./dist/node-firebot.d.ts",
    "./dist/node-firebot.mjs",
    "./dist/node-firebot.cjs",
    "./dist/node-firebot.mjs.map",
    "./dist/node-firebot.cjs.map"
  ],
  "scripts": {
    "build": "node build.js && dts-bundle-generator -o ./dist/node-firebot.d.ts ./src/index.ts",
    "prepublishOnly": "npm run build",
    "lint": "eslint --config eslint.config.mjs"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@tsconfig/node20": "^20.1.8",
    "@types/node": "^25.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/parser": "^8.50.0",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "dependencies": {
    "ws": "^8.18.3"
  }
}
